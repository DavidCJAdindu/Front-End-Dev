<!doctype html>

<html lang="en">

  <head>

    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- JQuery -->
    <script src="https://code.jquery.com/jquery-1.12.0.js" integrity="sha256-yFU3rK1y8NfUCd/B4tLapZAy9x0pZCqLZLmFL3AWb7s="
    crossorigin="anonymous"></script>

    <!-- CSS -->
    <link href="../assets/style.css" rel="stylesheet"> 

    <!-- Website Title -->
    <title>To-do List App</title>

  </head>

  <body>

    <img src="../assets/lavish-logo.png" alt="">

    <div id="todo-table">

      <form action="">
        <input type="text" name="item" placeholder="Add new item..." required>
        <button type="submit">Add item</button>       
      </form>

      <ul>

        <% todos.forEach(function(data) { %>

          <li><%= data.item %></li>

        <% }); %>

      </ul>

    </div>

    <script src="../assets/todo-list.js"></script>

  </body>

</html>